<project>
  <build>
    <plugins>
      <plugin>
   		<groupId>org.qooxdoo</groupId>
  		<artifactId>qooxdoo-maven-plugin</artifactId>
        <configuration>
        	<namespace>resources_app</namespace>
        	<sourcesDirectory>src/main/qooxdoo</sourcesDirectory>
        	<testDirectory>src/main/qooxdoo</testDirectory>
        	<resourcesDirectory>src/test/resources</resourcesDirectory>
        	<outputDirectory>target/test-target/qooxdoo</outputDirectory>
        	<cacheDirectory>target/test-target/qooxdoo/cache</cacheDirectory>
        	<translationDirectory>src/test/resources/translation</translationDirectory>
        	<encoding>UTF-8</encoding>
        	<sdkParentDirectory>target/test-target</sdkParentDirectory>
        	<config>config.json</config>
        	<project>
				<artifactId>test</artifactId>
            	<groupId>org.qooxdoo.test</groupId>
            	<version>0.0.1</version>
			</project>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>

