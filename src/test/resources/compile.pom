<project>
  <build>
    <plugins>
      <plugin>
   		<groupId>org.qooxdoo</groupId>
  		<artifactId>qooxdoo-maven-plugin</artifactId>
        <configuration>
        	<outputDirectory>target/test-target/qooxdoo</outputDirectory>
        	<namespace>compile_app</namespace>
        	<encoding>UTF-8</encoding>
        	<sdkParentDirectory>target/test-compile</sdkParentDirectory>
        	<config>config.json</config>
        	<useEmbeddedJython>true</useEmbeddedJython>
        	<buildJob>build</buildJob>
        	<jythonCompiledDirectory>target/test-compile</jythonCompiledDirectory>
        	<jythonTestCompiledDirectory>target/test-compile</jythonTestCompiledDirectory>
        	<project >
              <artifactId>test</artifactId>
              <groupId>org.qooxdoo.test</groupId>
              <version>0.0.1</version>
            </project>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>

